{
  "xsappname": "cd_cdlitepayrollupload_fs",
  "tenant-mode": "dedicated",
  "scopes": [
    {
      "name": "$XSAPPNAME.upload",
      "description": "Display and Upload files"
    },
    {
      "name": "$XSAPPNAME.delete",
      "description": "delete"
    },
    {
      "name": "$XSAPPNAME.approve",
      "description": "approve uploads for posting"
    },
    {
      "name": "$XSAPPNAME.admin",
      "description": "admin"
    }
  ],
  "attributes": [
    {
      "name": "companycode",
      "description": "companycode",
      "valueType": "s"
    }
  ],
  "role-templates": [
    {
      "name": "Accounting",
      "scope-references": [
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "accounting user role",
      "attribute-references": [
        "companycode"
      ]
    },
    {
      "name": "Accounting_US",
      "scope-references": [
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "US accounting user role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "US10"
          ]
        }
      ]
    },
    {
      "name": "Accounting_CA",
      "scope-references": [
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "CA accounting user role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "CA10"
          ]
        }
      ]
    },
    {
      "name": "Accounting_DE",
      "scope-references": [
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "DE accounting user role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "DE10"
          ]
        }
      ]
    },
    {
      "name": "Accounting_GB",
      "scope-references": [
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "GB accounting user role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "GB10"
          ]
        }
      ]
    },
    {
      "name": "Approver",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.approve",
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "approver role",
      "attribute-references": [
        "companycode"
      ]
    },
    {
      "name": "Approver_NA",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.approve",
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "NA approver role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "US10",
            "CA10"
          ]
        }
      ]
    },
    {
      "name": "Approver_EU",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.approve",
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "EU approver role",
      "attribute-references": [
        {
          "name": "companycode",
          "default-values": [
            "GB10",
            "DE10"
          ]
        }
      ]
    },
    {
      "name": "Admin",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.upload",
        "$XSAPPNAME.delete"
      ],
      "description": "admin role"
    }
  ],
  "role-collections": [
    {
      "name": "ZBC:APP_PAB:UPLD_US",
      "description": "US accounting user role",
      "role-template-references": [
        "$XSAPPNAME.Accounting_US"
      ]
    },
    {
      "name": "ZBC:APP_PAB:UPLD_CA",
      "description": "CA accounting user role",
      "role-template-references": [
        "$XSAPPNAME.Accounting_CA"
      ]
    },
    {
      "name": "ZBC:APP_PAB:UPLD_DE",
      "description": "DE accounting user role",
      "role-template-references": [
        "$XSAPPNAME.Accounting_DE"
      ]
    },
    {
      "name": "ZBC:APP_PAB:UPLD_GB",
