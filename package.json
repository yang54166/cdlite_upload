{
  "name": "cd_compensationdetails_c",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "express": "^4",
    "hdb": "^0.19.0"
  },
  "devDependencies": {
    
  },
  "scripts": {
    "start": "cds run",
    "build": "mbt build --mtar $npm_package_name-$npm_package_version.mtar",
    "login": "cf login -a https://api.cf.eu10.hana.ondemand.com --sso",
    "deploy": "cf deploy mta_archives/$npm_package_name-$npm_package_version.mtar",
    "deploy-local": "cds deploy --profile hybrid",
    "start-local": "cds watch --profile hybrid",
    "undeploy": "cf undeploy cd_conpensationdetails_c --delete-services --delete-service-keys"
  },
  "engines": {
    "node": "^16.15"
  },
  "cds": {
    "build":{
      "tasks": [
        {
          "for": "hana",
          "dest": "../db"
        },
        {  "for": "node-cf" }
      ]
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  }
}