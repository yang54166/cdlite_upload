      "role-template-references": [
        "$XSAPPNAME.Accounting_GB"
      ]
    },
    {
      "name": "ZBC:APP_PAB:APR_NA",
      "description": "NA approver role",
      "role-template-references": [
        "$XSAPPNAME.Approver_NA"
      ]
    },
    {
      "name": "ZBC:APP_PAB:APR_EU",
      "description": "EU approver role",
      "role-template-references": [
        "$XSAPPNAME.Approver_EU"
      ]
    },
    {
      "name": "ZBC:APP_PAB:ADM",
      "description": "Global admin role",
      "role-template-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ],
  "oauth2-configuration": {
    "redirect-uris": [
      "https://**.cfapps.eu10.hana.ondemand.com/login/callback",
      "https://**.eu10.applicationstudio.cloud.sap/login/callback",
      "https://oauth.pstmn.io/v1/callback"
    ]
  }
